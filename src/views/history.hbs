<main>
    <h3>Hey {{name}}, here is your feedback history</h3>

    {{#if arr}}
    {{#each arr}}
    <form action="/delete-record={{id}}" onsubmit="return anchorLink()" method="post">
        <ul>

            <li>id: {{id}}</li>
            <li>Gym: {{gym_name}}</li>
            <li>Duration: {{workout_duration}}</li>
            <li>Favourite exercise: {{favourite_workout}}</li>
            <button class="delete-buttons" name={{id}} onclick="return anchorLink">Delete Feedback</button>


        </ul>


    </form>
    {{/each}}

    {{else}}

    <p>You have no feedback history</p>
    {{/if}}
</main>